{
  "manifest_version": 2,
  "name": "dliste-image-filter",
  "version": "1.0",

  "description": "canvas filter from dlsite Play",

  "icons": {
    "16": "icons/filter-16.png",
    "32": "icons/filter-32.png",
    "48": "icons/filter-48.png",
    "128": "icons/filter-148.png"
  },

  "content_scripts": [
    {
      "matches": ["*://play.dlsite.com/*"],
      "js": ["main.js"],
      "css": ["main.css"]
    }
  ]
}
